<?xml version="1.0"?>
<!DOCTYPE package-info SYSTEM "http://www.elkarte.net/site/package-info">
<package-info xmlns="http://www.elkarte.net/site/package-info" xmlns:elk="http://www.elkarte.net/">
	<name>MySQLCache</name>
	<id>tinoest:MySQLCache</id>
	<type>addon</type>
	<version>1.0.2</version>

	<install for="1.1 RC1 - 1.1.99">
		<require-file name="Mysqlbased.php" destination="SUBSDIR/CacheMethod">Install the MySQL Cache</require-file>
		<redirect url="index.php?action=admin;area=corefeatures" timeout="5000">redirect.txt</redirect>
	</install>

	<uninstall for="1.1 RC1 - 1.1.99">
		<remove-file name="SUBSDIR/CacheMethod/Mysqlbased.php" />
	</uninstall>

</package-info>
